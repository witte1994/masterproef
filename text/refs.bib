abstract = {A problem that I often find on dashboards is senseless variety in the choice of display mechanisms. When deciding how to best display each section of data, you should ask, "Which means of display will communicate this data most effectively and efficiently?" Many dashboards look as if the designer instead asked, "Which type of display have I not already used on this dashboard?" and then selected something new for the sake of variety.},
author = {Few, Stephen},
isbn = {1521-2912},
issn = {15212912},
